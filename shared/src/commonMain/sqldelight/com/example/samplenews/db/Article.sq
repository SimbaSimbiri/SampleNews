CREATE TABLE Article(
    title TEXT NOT NULL,
    description TEXT,
    date TEXT NOT NULL,
    imageUrl TEXT,
    publisher TEXT NOT NULL,
    author TEXT,
    urlToPage TEXT NOT NULL
);

insertArticle:
INSERT INTO Article(title, description, date, imageUrl, publisher,author,urlToPage)
VALUES (?,?,?,?,?,?,?);

selectAllArticles:
SELECT * FROM Article;

removeAllArticles:
DELETE FROM Article;